{
    "name": "renderpdf-io/renderpdf-laravel",
    "type": "library",
    "version": "1.0.0",
    "description": "Render and covert HTML to PDF using RenderPDF.io service, library for Laravel",
    "keywords": [
        "laravel library",
        "laravel convert html to pdf",
        "laravel render html to pdf",
        "html to pdf",
        "renderpdf",
        "renderpdf.io",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "laravel/framework": "^9|^10|^11|dev-master"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10",
        "orchestra/testbench": "^7|^8|^9",
        "phpunit/php-code-coverage": "^10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RenderPdfIo\\RenderPdfIoServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "RenderPdfIo\\": "src/"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "RenderPdfIo\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test-coverage": [
            "@php vendor/bin/phpunit --coverage-clover coverage.xml"
        ],
        "test": [
            "@php vendor/bin/phpunit"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
